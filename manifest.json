{
    "manifest_version": 3,
    "name": "Password Generator",
    "version": "1.0",
    "description": "Генератор паролей с настройками.",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "./icons/icon16.png",
            "32": "./icons/icon32.png",
            "44": "./icons/icon44.png",
            "48": "./icons/icon48.png",
            "64": "./icons/icon64.png",
            "128": "./icons/icon128.png"
        }
    },
    "permissions": ["activeTab"]
}
